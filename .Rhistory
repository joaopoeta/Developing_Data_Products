e <- rnorm(N, 0, 2)
y<-0.5+2*x+e
summary(y)
plot(x, y)
#Correlation and Covariance
cor(x,y)
cov(x,y)
#Running the Regression
# The regression command is lm for linear model. We will store that model in a variable called model. The order of the variables is dependent followed by a tilde "~" followed by a list of independent variables.
model = lm(y ~ x)
model
summary(model)
abline(model)
s <- seq(length(x)-1)  # one shorter than data
#FIM <- predict(model, newdata=data.frame(X=x))
#segments(x[s], y[s], x[s], FIM, col= 'red')
# Constructing the dynamic line
XMIN <- min(x)
XMAX <- max(x)
xExtrem <- c(XMIN, XMAX)
yExtrem <- predict(model, newdata = data.frame(X = xExtrem))
points(xExtrem, yExtrem, type='l')
xExtrem
yExtrem
yExtrem <- predict(model, newdata = data.frame(x = xExtrem))
yExtrem
